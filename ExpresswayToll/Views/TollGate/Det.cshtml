@model Model.t_TollGate
<div class="modal-body">
    <div class="col-md-4"></div>
    <div class="">
        <h3>
            <p>
                <span>编号：</span>
                <span>@Model.Id</span>
            </p>
            <p>
                <span>@Html.DisplayNameFor(c => c.TollGate)：</span>
                <span>@Model.TollGate</span>
            </p>
            <p>
                <span>@Html.DisplayNameFor(c => c.Loction)：</span>
                <span>@Model.Loction</span>
            </p>
            <p>
                <span>@Html.DisplayNameFor(c => c.Count)：</span>
                <span>@Model.Count</span>
            </p>
            <p>
                <span>所在道路：</span>
                <span>@Model.road.RoadName</span>
            </p>
        </h3>

    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
    @*<button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("Edit", "User", new { Id =@Model.Id })'">编辑</button>*@
    <button type="submit" class="btn btn-primary" onclick="location.href ='@Url.Action("Del", new { id =@Model.Id })'">删除</button>
</div>
