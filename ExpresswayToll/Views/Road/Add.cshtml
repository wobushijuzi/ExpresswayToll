@model Model.t_Road
@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="portlet light bordered">
    <div class="portlet-title">
        <div class="caption">
            <i class="icon-equalizer font-red-sunglo"></i>
            <span class="caption-subject font-red-sunglo bold uppercase">添加道路 </span>
        </div>
    </div>
    <div class="portlet-body form">
        <form action="Add" method="post" class="form-horizontal">
            <div class="form-body">
                <div class="form-group">
                    <label class="col-md-3 control-label">道路编号</label>
                    <div class="col-md-4">
                        <div class="input-icon">
                            <i class="fa fa-bell-o"></i>
                            @Html.TextBoxFor(c => c.RoadNo, new { @class = "form-control", placeholder = "道路名称" })
                            @*<input type="text" class="form-control" name="Start" placeholder="开始点">*@
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">道路名</label>
                    <div class="col-md-4">
                        <div class="input-icon">
                            <i class="fa fa-bell-o"></i>
                            @Html.TextBoxFor(c => c.RoadName, new { @class = "form-control", placeholder = "道路名称" })
                            @*<input type="text" class="form-control" name="End" placeholder="结束点">*@
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">建造公司</label>
                    <div class="col-md-4">
                        <div class="input-icon">
                            <i class="fa fa-bell-o"></i>
                            @Html.TextBoxFor(c => c.BuildCompany, new { @class = "form-control", placeholder = "建造公司" })
                            @*<input type="text" class="form-control" name="Distance" placeholder="距离">*@
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-3 control-label">建造时间</label>
                    <div class="col-md-4">
                        <div class="input-icon">
                            <i class="fa fa-bell-o"></i>
                            @Html.TextBoxFor(c => c.BuildTime, new { @class = "form-control", placeholder = "建造时间" })
                            @*<input type="text" class="form-control" name="Distance" placeholder="距离">*@
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label">备注</label>
                    <div class="col-md-4">
                        <div class="input-icon">
                            <i class="fa fa-bell-o"></i>
                            @Html.TextBoxFor(c => c.Remark, new { @class = "form-control", placeholder = "备注" })
                            @*<input type="text" class="form-control" name="Remark" placeholder="备注">*@
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-actions">
                <div class="row">
                    <div class="col-md-offset-3 col-md-9">
                        <button type="submit" class="btn green">提交</button>
                        <button type="button" class="btn default" onclick="location.href='@Url.Action("Index")'">取消</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>

